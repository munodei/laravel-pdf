{
    "name": "niklasravnsborg/laravel-pdf",
    "description": "A simple Laravel service provider for including the mPDF library.",
    "keywords": ["laravel", "pdf", "mpdf", "service provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Ravnsborg",
            "email": "mail@niklasravnsborg.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "dompdf/dompdf": "^1.0",
        "mpdf/mpdf": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Niklasravnsborg\\LaravelPdf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niklasravnsborg\\LaravelPdf\\PdfServiceProvider"
            ]
        }
    }
}
